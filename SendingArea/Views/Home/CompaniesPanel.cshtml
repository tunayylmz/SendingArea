    <script type="text/javascript">
        $(document).ready(function () {
            function wk_notify() {
                $.ajax({
                    url: 'index.php?route=account/customerpartner/notification/notifications&json_notification=true',
                    type: 'get',
                    methodType: 'json',
                    success: function (data) {

                        if (data['notification_total']) {
                            if (data['separate_view']) {
                                $("#notification").html('<span class="label label-danger pull-left">' + data['notification_total'] + '</span> <img src="admin/view/image/notify.png" title="Bildirimler">');
                            } else {
                                $("#notification").html('Bildirimler' + ' <span class="badge">' + data['notification_total'] + '<span>');
                            }
                        } else {
                            if (data['separate_view']) {
                                $("#notification").html('<img src="admin/view/image/notify.png" title="Bildirimler">');
                            } else {
                                $("#notification").text('Bildirimler');
                            }
                        }

                        $("span").each(function (index, element) {
                            var span_id = $(this).attr('id');
                            if (span_id && data[span_id]) {
                                $("#" + span_id).text(data[span_id]);
                            }
                        });

                        $("ul").each(function (index, element) {
                            var ul_id = $(this).attr('id');
                            if (ul_id && data[ul_id]) {
                                var html = '';
                                for (var i = 0; i < data[ul_id].length; i++) {
                                    html += '<li>' + data[ul_id][i] + '</li>';
                                }
                                if (ul_id == 'seller_product_reviews') {
                                    html += '<li style="display: block; overflow: auto;"><a href="https://pazaryeri.demoist.site/index.php?route=account/customerpartner/notification&tab=product">Hepsini gör</a></li>';
                                } else if (ul_id == 'seller_reviews') {
                                    html += '<li style="display: block; overflow: auto;"><a href="https://pazaryeri.demoist.site/index.php?route=account/customerpartner/notification&tab=seller">Hepsini gör</a></li>';
                                } else {
                                    html += '<li style="display: block; overflow: auto;"><a href="https://pazaryeri.demoist.site/index.php?route=account/customerpartner/notification">Hepsini gör</a></li>';
                                }
                                $("#" + ul_id).html(html);
                            }
                        });
                    }
                });
            }
            setInterval(wk_notify, 30000);
            wk_notify();
        });
    </script>






