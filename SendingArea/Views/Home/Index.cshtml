                                    <form id="distance_form" action="/Scripts/System Js/GoogleMaps.js">
                                        <div class="form-group">
                                            <div class="form-group">
                                                <div class="col-sm-5"> <label class="title-2"> Konum Bilgileri : </label></div>
                                                <div class="col-sm-9">
                                                    <div class="form-group ">
                                                        <input id="from_places" class="form-control" placeholder="Nereden" />
                                                        <input id="origin" type="hidden" name="origin" required />
                                                    </div>
                                                    <div class="form-group ">
                                                        <input id="to_places" class="form-control" placeholder="Nereye" />
                                                        <input id="destination" type="hidden" name="destination" required />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-9 col-xs-12 pull-left" style="display:none;" id="Km">
                                                <div class="btn-1">
                                                    <span> toplam mesafe : </span>
                                                    <span class="btn-1 dark">
                                                        <span id='in_kilo'></span> KM <span></span>
                                                    </span>
                                                </div>
                                            </div>
                                            <div class="form-group wow fadeInUp">
                                                <div class="col-sm-4"> <label class="title-2"> Kargo Tipi : </label></div>
                                                <div class="col-sm-9">
                                                    <div class="form-group">
                                                        <select class=" form-control" title="Kargo Tipini Seçin" id="Kargo_Tipi">
                                                            <option value="KargoTip">Kargo Tipini Seçin</option>
                                                            <option value="Evrak">Evrak ve benzeri</option>
                                                            <option value="Koli">Koli ve benzeri</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div><br />
                                            <div class="dv-order-block-with-error col-sm-9" id="evrak_ozellikleri" style="display:none;">
                                                <div class="dv-order-tabs dv-order-tabs--horizontal ">
                                                    <div class="dv-order-tabs__tab dv-order-tabs__tab--active" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label dv-order-tabs__tab-label--active">
                                                            0 - 1 kg
                                                        </span>
                                                    </div>
                                                    <div class="dv-order-tabs__tab" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label">
                                                            1 - 5 kg
                                                        </span>
                                                    </div>
                                                    <div class="dv-order-tabs__tab" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label">
                                                            5 - 10 kg
                                                        </span>
                                                    </div>
                                                    <div class="dv-order-tabs__tab" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label">
                                                            10 - 15 kg
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="dv-order-block-with-error col-sm-9" id="koli_ozellikleri" style="display:none;">
                                                <div class="dv-order-tabs dv-order-tabs--horizontal ">
                                                    <div class="dv-order-tabs__tab dv-order-tabs__tab--active col-sm-9" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label dv-order-tabs__tab-label--active">
                                                            0 - 30 kg
                                                        </span>
                                                    </div>
                                                    <div class="dv-order-tabs__tab col-sm-9" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label">
                                                            30 - 60 kg
                                                        </span>
                                                    </div>
                                                    <div class="dv-order-tabs__tab col-sm-9" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label">
                                                            60 - 99 kg
                                                        </span>
                                                    </div>
                                                    <div class="dv-order-tabs__tab col-sm-9" style="text-align:center;">
                                                        <span class="dv-order-tabs__tab-label">
                                                            99-150 kg
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group wow fadeInUp" id="koli_ozellikleri" style="display:none;">
                                                <div class="col-sm-5"> <label class="title-2"> Kargo Özellikleri : </label></div>
                                                <div class="form-group wow fadeInUp">
                                                    <div class="col-sm-9"> <input type="text" placeholder="Yükseklik (cm)" class="form-control"> </div>
                                                </div>
                                                <div class="form-group wow fadeInUp">
                                                    <div class="col-sm-9"> <input type="text" placeholder="Genişlik (cm)" class="form-control"> </div>
                                                </div>
                                                <div class="form-group wow fadeInUp">
                                                    <div class="col-sm-9"> <input type="text" placeholder="Derinlik (cm)" class="form-control"> </div>
                                                </div>
                                                <div class="form-group wow fadeInUp">
                                                    <div class="col-sm-9"> <input type="text" placeholder="Ağırlık (kg)" class="form-control"> </div>
                                                </div>
                                            </div>
                                            <div class="form-group wow fadeInUp">
                                                <div class="col-sm-4"> <label class="title-2"> Kurye Tipi : </label></div>
                                                <div class="col-sm-9">
                                                    <div class="form-group">
                                                        <select class=" form-control" title="Kurye Tipini Seçin">
                                                            <option value="KuryeTip">Kurye Tipini Seçin</option>
                                                            <option value="Araba">Araba</option>
                                                            <option value="Motor">Motorsiklet</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group wow fadeInUp">
                                                <div class="col-sm-9 col-xs-12 pull-left">
                                                    <input class="btn-1" style="text-align:center;" type="submit" onclick="KmHesabi('Km')" value="Kuryenizi Bulun" />
                                                </div>
                                            </div>
                                        </div>
                                    </form>

                                    <script>
                                        function KmHesabi(Km) {
                                            var secilenID = document.getElementById(Km);
                                            if (secilenID.style.display == "none") {
                                                secilenID.style.display = "";
                                            } else {
                                                secilenID.style.display = "none";
                                            }
                                        }
                                    </script>
