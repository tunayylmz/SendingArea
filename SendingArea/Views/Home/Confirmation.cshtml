
@{
    ViewBag.Title = "Confirmation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/Css/Account.css" rel="stylesheet" />
<section class="banner mask-overlay pad-120 white-clr" style="padding-bottom: 0px; padding-top: 0px;">
    <div class="grey" style="padding:0px; margin-top:2%;">
        <div class="container">
            <div class="topheader">
                <h1 style="text-align: left;">
                    <font style="vertical-align: inherit;">
                        <font style="vertical-align: inherit; color: white;">
                            Kargo Doğrulama
                        </font>
                    </font>
                </h1>
                <div class="breadcrumbs" style="float:left;">
                    <ul class="list-inline">
                        <li>
                            <a href="Index">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">
                                        Anasayfa
                                    </font>
                                </font>
                            </a>
                            <font style="vertical-align: inherit;">
                                <font style="vertical-align: inherit;">
                                    /
                                </font>
                            </font>
                        </li>
                        <li>
                            <span class="current">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">
                                        Kargo Doğrulama
                                    </font>
                                </font>
                            </span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="pad-50 visible-lg"></div>
</section>
<div class="section white page-content" id="MyAccount">
    <div class="container">
        <div id="content" class="col-lg-9 col-md-9 col-sm-12 col-xs-12">
            <div class="post-contents">
                <div class="woocommerce">
                    <div class="woocommerce-notices-wrapper">
                    </div>
                    <h2>
                        <font style="vertical-align: inherit;">
                            <font style="vertical-align: inherit;">
                                Kargonuzu Onaylayın
                            </font>
                        </font>
                    </h2>
                    <form class="woocommerce-form woocommerce-form-login login" method="post" action="Payment">
                        <h3>Adres bilgileri</h3>
                        <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                            <label for="adressFrom">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">Kargo teslim alış adresi &nbsp; </font>
                                </font><span class="required">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            *
                                        </font>
                                    </font>
                                </span>
                            </label>
                            <input type="text" class="woocommerce-Input woocommerce-Input--text input-text" name="adressFrom" id="adressFrom" autocomplete="adressFrom" value="">
                        </p>
                        <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                            <label for="adressTo">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">Kargo teslimat adresi &nbsp; </font>
                                </font><span class="required">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            *
                                        </font>
                                    </font>
                                </span>
                            </label>
                            <input type="text" class="woocommerce-Input woocommerce-Input--text input-text" name="adressTo" id="adressTo" autocomplete="adressTo" value="">
                        </p><br />
                        <h3>Gönderen ve alıcı bilgileri</h3>
                        <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                            <label for="nameFrom">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">Kargo sahibi adı - soyadı &nbsp; </font>
                                </font><span class="required">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            *
                                        </font>
                                    </font>
                                </span>
                            </label>
                            <input type="text" class="woocommerce-Input woocommerce-Input--text input-text" name="nameFrom" id="nameFrom" autocomplete="nameFrom" value="">
                        </p>
                        <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                            <label for="tcNo">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">Kargo sahibi T.C kimlik numarası &nbsp; </font>
                                </font><span class="required">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            *
                                        </font>
                                    </font>
                                </span>
                            </label>
                            <input type="text" class="woocommerce-Input woocommerce-Input--text input-text" name="tcNo" id="tcNo" autocomplete="tcNo" value="">
                        </p>
                        <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                            <label for="nameTo">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">Alıcı adı - soyadı &nbsp; </font>
                                </font><span class="required">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            *
                                        </font>
                                    </font>
                                </span>
                            </label>
                            <input type="text" class="woocommerce-Input woocommerce-Input--text input-text" name="nameTo" id="nameTo" autocomplete="nameTo" value="">
                        </p><br />
                        <h3>Taşıyıcı firma ve kargo bilgileri</h3>
                        <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                            <label for="carrier">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">Taşıyıcı firma &nbsp; </font>
                                </font><span class="required">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            *
                                        </font>
                                    </font>
                                </span>
                            </label>
                            <input type="text" class="woocommerce-Input woocommerce-Input--text input-text" name="carrier" id="carrier" autocomplete="carrier" value="">
                        </p>
                        <p class="woocommerce-form-row woocommerce-form-row--wide form-row form-row-wide">
                            <label for="deliveyDay">
                                <font style="vertical-align: inherit;">
                                    <font style="vertical-align: inherit;">Kargo teslim alış tarih / saat &nbsp; </font>
                                </font><span class="required">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            *
                                        </font>
                                    </font>
                                </span>
                            </label>
                            <input type="text" class="woocommerce-Input woocommerce-Input--text input-text" name="deliveyDay" id="deliveyDay" autocomplete="deliveyDay" value="">
                        </p>

                        <p class="form-row">
                            <input type="hidden" id="woocommerce-login-nonce" name="woocommerce-login-nonce" value="38efa7f48d">
                            <input type="hidden" name="_wp_http_referer" value="/themes/gocourierwp/my-account/">
                            <br />
                            <button style="background-color:#F5AB35;" type="submit" class="woocommerce-button button woocommerce-form-login__submit" name="login" value="Log in">
                                <font style="vertical-align: inherit; color:white ">
                                    <font style="vertical-align: inherit;">
                                        Kargonuzu Onaylayın
                                    </font>
                                </font>
                            </button>
                            <br /><br />
                        </p>
                        @*<p class="woocommerce-LostPassword lost_password">
                                <a href="http://event-theme.com/themes/gocourierwp/my-account/lost-password/">
                                    <font style="vertical-align: inherit;">
                                        <font style="vertical-align: inherit;">
                                            Şifreni mi unuttun?
                                        </font>
                                    </font>
                                </a>
                            </p>*@
                    </form>
                </div>
            </div>
        </div>
        <div id="sidebar" class="col-md-3 col-sm-12 col-xs-12">

        </div>
    </div>
</div>

